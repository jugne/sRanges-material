			<sequence spec='Sequence' taxon='t3_first' value='?'/>
			<sequence spec='Sequence' taxon='t3_last' value='?'/>
			<sequence spec='Sequence' taxon='t5_first' value='?'/>
        </data>

        <siteModel spec='SiteModel' id="siteModel_dna" gammaCategoryCount='1'>
            <substModel spec='JukesCantor' id='JC69'/>
        </siteModel>

        <branchRateModel id="StrictClock_dna" spec="beast.base.evolution.branchratemodel.StrictClockModel">
            <parameter dimension="1" estimate="false" id="clockRate_dna" minordimension="1" name="clock.rate" value="0.003"/>
        </branchRateModel>
    </run>


