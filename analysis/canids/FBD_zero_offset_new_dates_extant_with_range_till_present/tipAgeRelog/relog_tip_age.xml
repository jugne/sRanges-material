<beast version='2.0' namespace='beast.base.inference.parameter
                                :beast.base.evolution.operator
                                :feast
                                :feast.expressions
                                :feast.fileio.logfileiterator
                                :sr.evolution.tree'>


     <run spec="LogFileIterator">

        <logFileState spec="TreeLogFileState" logFileName="../canidae_rho_zero.trees">
            <tree spec="beast.base.evolution.tree.Tree" id="tree" >
            </tree>
        </logFileState>

        <logger fileName="canidae_rho_zero.tipAge.log" id="specLog" logEvery="50000" sanitiseHeaders="true" sort="smart">
            <log spec="sr.evolution.tree.TipAgeLogger" simpleTree="@tree"/>
        </logger>
    </run>
</beast>