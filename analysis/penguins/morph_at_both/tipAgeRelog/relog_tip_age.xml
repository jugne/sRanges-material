                                :beast.base.evolution.operator
                                :feast
                                :feast.expressions
                                :feast.fileio.logfileiterator
                                :sr.evolution.tree'>


     <run spec="LogFileIterator">

        <logFileState spec="TreeLogFileState" logFileName="../penguins_inf_morph_at_both.combined.trees">
            <tree spec="sr.evolution.tree.SRTree" id="tree" nodetype="sr.evolution.tree.SRNode">
            </tree>
